#include<iostream>
using namespace std;
int main(){
    int count;
    int i,j;
    cout<<"enter the length of the array"<<endl;
    cin>>count;
    int input[count],output[count];
    for(i=0;i<count;i++){
        cin>>input[i];
    }
    for(i=0;i<count;i++){
        output[i]=input[count-i-1];
        cout<<output[i]<<" ";
    }
    
}
